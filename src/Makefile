			(cd Pair; make all)
			(cd Stack; make all)
			(cd List; make all)
			(cd Vec; make all)
			(cd Queue; make all)
			(cd Deque; make all)
			(cd PriorityQueue; make all)
			(cd Set; make all)

clean:
			(cd Pair; make clean)
			(cd Stack; make clean)
			(cd List; make clean)
			(cd Vec; make clean)
			(cd Queue; make clean)
			(cd Deque; make clean)
			(cd PriorityQueue; make clean)
			(cd Set; make clean)
